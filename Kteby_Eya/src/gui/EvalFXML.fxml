      <TextField fx:id="idevalTF" layoutX="499.0" layoutY="147.0" prefHeight="25.0" prefWidth="59.0" />
      <Label fx:id="lab_title_eval" layoutX="136.0" layoutY="4.0" prefHeight="42.0" prefWidth="345.0" text="            Gestion des évaluations" textFill="#1630b2">
         <font>
            <Font name="Broadway" size="19.0" />
         </font>
      </Label>
      <Button layoutY="4.0" mnemonicParsing="false" onAction="#goAffAction" prefHeight="17.0" prefWidth="157.0" text="Consulter les évaluations">
         <font>
            <Font name="Calibri Bold" size="12.0" />
         </font>
      </Button>
      <Hyperlink layoutX="483.0" onAction="#returnAction" text="Diriger vers catégorie &gt;&gt;" textFill="#2c2551">
         <font>
            <Font name="Calibri Bold Italic" size="12.0" />
         </font></Hyperlink>
