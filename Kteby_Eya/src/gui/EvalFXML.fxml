<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EvalFXMLController">
   <children>
      <ImageView fitHeight="406.0" fitWidth="622.0" layoutX="-3.0" layoutY="1.0" pickOnBounds="true">
         <image>
            <Image url="@../images/book2.jpg" />
         </image>
      </ImageView>
      <Label layoutX="40.0" layoutY="100.0" prefHeight="17.0" prefWidth="109.0" text="Nom de Livre :">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font></Label>
      <Label layoutX="28.0" layoutY="159.0" prefHeight="25.0" prefWidth="109.0" text="Nombre des stars :">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font></Label>
      <TextField fx:id="nomlivTF" layoutX="24.0" layoutY="125.0" prefHeight="25.0" prefWidth="109.0" />
      <TextField fx:id="nbstarsTF" layoutX="40.0" layoutY="193.0" prefHeight="25.0" prefWidth="77.0" />
      <Button layoutX="162.0" layoutY="121.0" mnemonicParsing="false" onAction="#submitAction" prefHeight="25.0" prefWidth="77.0" text="Ajouter" underline="true">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font></Button>
      <Button layoutX="162.0" layoutY="172.0" mnemonicParsing="false" onAction="#updateAction" prefHeight="25.0" prefWidth="77.0" text="Modifier" underline="true">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font></Button>
      <Button layoutX="490.0" layoutY="191.0" mnemonicParsing="false" onAction="#deleteAction" prefHeight="25.0" prefWidth="77.0" text="Supprimer" underline="true">
         <font>
            <Font name="Calibri Bold" size="12.0" />
         </font></Button>
      <Label layoutX="493.0" layoutY="113.0" prefHeight="17.0" prefWidth="123.0" text=" Evaluation :">
         <font>
            <Font name="Calibri Bold" size="13.0" />
